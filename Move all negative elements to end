import java.util.*;
public class Main
{
	public static void main(String[] args) 
	{
	   int sw =0;
	   Scanner sc = new Scanner (System.in);
	   int n = sc.nextInt();
	   int arr[] = new int[n];
	   ArrayList <Integer> ar = new ArrayList <Integer>();
	   for (int i =0;i<n;i++)
	   {
	       arr[i]=sc.nextInt();
	       ar.add(arr[i]);
	   }
	   Collections.sort(ar);
	   for (int i=0;i<n;i++)
	   {
	     System.out.print(ar.get(i));
	   }
	   
	}
}
